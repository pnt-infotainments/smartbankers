<p id="notice"><%= notice %></p>

<h1>Listing Subscribers</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Type</th>
      <th colspan="3">Action</th>
    </tr>
  </thead>

  <tbody>
    <% @subscribes.each do |subscribe| %>
      <tr>
        <td><%= subscribe.user %></td>
        <td><%= subscribe.type %></td>
        <td><%= link_to 'Show', administrator_subscribe_path(subscribe) %></td>
        <td><%= link_to 'Edit', edit_administrator_subscribe_path(subscribe) %></td>
        <td><%= link_to 'Destroy', administrator_subscribe_path(subscribe), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

